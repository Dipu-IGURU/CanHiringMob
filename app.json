{
  "expo": {
    "name": "CanHiring Solutions",
    "slug": "canhiring-solutions",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "icon": "./assets/512.jpg",
    "splash": {
      "image": "./assets/512.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/512.jpg",
      "supportsTablet": true,
      "bundleIdentifier": "com.iguru.canhiringsolutions"
    },
    "android": {
      "icon": "./assets/512.jpg",
      "adaptiveIcon": {
        "foregroundImage": "./assets/512.jpg",
        "backgroundColor": "#2563EB"
      },
      "package": "com.iguru.canhiringsolutions",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "READ_MEDIA_IMAGES"
      ],
      "blockedPermissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/canhirngfavicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-google-signin/google-signin"
    ],
    "privacy": "public",
    "extra": {
      "eas": {
        "projectId": "4b808f71-6146-422e-b746-6d5eb6b74fbe"
      }
    },
    "owner": "dipu-i-guru"
  }
}
