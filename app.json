{
  "expo": {
    "name": "CanHiring Solutions",
    "slug": "canhiring-solutions",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "icon": "./assets/512.jpg",
    "splash": {
      "image": "./assets/512.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/512.jpg",
      "supportsTablet": true,
      "bundleIdentifier": "com.iguru.canhiringsolutions"
    },
    "android": {
      "icon": "./assets/512.jpg",
      "adaptiveIcon": {
        "foregroundImage": "./assets/512.jpg",
        "backgroundColor": "#2563EB"
      },
      "package": "com.can.canhiring",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA"
      ],
      "blockedPermissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "canhiringmob.onrender.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/canhirngfavicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": false,
          "cameraPermission": "The app accesses your camera to let you take photos for your profile picture."
        }
      ]
    ],
    "privacy": "public",
    "extra": {
      "eas": {
        "projectId": "d2a35955-1a49-46e8-8538-478d76340742"
      }
    },
    "scheme": "canhiring",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "i-guru"
  }
}
